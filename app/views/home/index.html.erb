

<%= render partial: 'shared/branding_modal_header' %>

<div class="container mt-4 mb-5">
  <div class="container mb-5 text-white bg-primary rounded-5 text-center" style="width:400px;">
    <h4 class=""><%= @all_users.size %> users registered</h4>
  </div>
</div>

<div class="d-flex mb-5">
  <% @all_properties.each do |property|  %>
    <div>
      <p><%= property.name %></p>
      <p><%= property.headline %></p>
      <p><%= property.description %></p>
      <p><%= property.address_1 %></p>
      <p><%= property.address_2 %></p>
      <p><%= property.city %></p>
      <p><%= property.state %></p>
      <p><%= property.country %></p>
    </div>
  <% end %>
</div>
